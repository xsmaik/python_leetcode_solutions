class Solution:
    def unequalTriplets(self, nums: List[int]) -> int:
        total = 0

        for i in range(len(nums)):
            for j in range(i + 1, len(nums)):
                for k in range(j + 1, len(nums)):
                    if nums[i] != nums[j] != nums[k] != nums[i]:
                        total += 1
        
        return total
