from typing import List

class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        P = []

        for i in range(rowIndex + 1):
            row = [1] * (i + 1)
            for j in range(1, i):
                row[j] = P[i - 1][j - 1] + P[i - 1][j]
            P.append(row)

        return P[rowIndex]
