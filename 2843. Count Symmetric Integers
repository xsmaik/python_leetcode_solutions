class Solution:
    def countSymmetricIntegers(self, low: int, high: int) -> int:
        total = 0

        for i in range(low, high + 1):
            num = str(i)
            n = len(num)

            if n % 2 != 0:
                continue

            pol = n // 2

            left = sum(int(num[i]) for i in range(pol))
            right = sum(int(num[i]) for i in range(pol, n))

            if left == right:
                total += 1

        return total
