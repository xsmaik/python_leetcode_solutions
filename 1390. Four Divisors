class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        total = 0

        for n in nums:
            divs = set()
            j = 1

            while j * j <= n:
                if n % j == 0:
                    divs.add(j)
                    divs.add(n // j)
                if len(divs) > 4:
                    break
                j += 1

            if len(divs) == 4:
                total += sum(divs)

        return total
