class Solution:
    def findMissingAndRepeatedValues(self, grid: List[List[int]]) -> List[int]:
        arr = []
        res = []

        for i in range(len(grid)):
            for j in range(len(grid[i])):
                arr.append(grid[i][j])

        for i in range(len(arr)):
            if arr.count(arr[i]) > 1:
                res.append(arr[i])
                break

        for i in range(1, len(arr) + 1):
            if i not in arr:
                res.append(i)

        return res
