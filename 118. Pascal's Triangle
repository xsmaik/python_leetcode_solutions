class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        P = []

        for i in range(numRows):
            row = [1] * (i + 1)
            for j in range(i + 1):
                if j != 0 and j != i:
                    row[j] = P[i-1][j-1] + P[i-1][j]

            P.append(row)

        return P
