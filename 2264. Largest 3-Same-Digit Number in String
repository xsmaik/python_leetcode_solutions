class Solution:
    def largestGoodInteger(self, num: str) -> str:
        res = []

        for i in range(2, len(num)):
            total = ''
            if num[i - 2] == num[i - 1] == num[i]:
                total += num[i - 2] + num[i - 1] + num[i]
                res.append(total)

        if res:
            return max(res)
        else:
            return ""
