class Solution:
    def separateDigits(self, nums: List[int]) -> List[int]:
        res = []
        arr = []

        for i in range(len(nums)):
            if nums[i] > 10:
                lst = list(str(nums[i]))
                res.append(lst)
            else:
                res.append(str(nums[i]))

        for i in res:
            for j in i:
                arr.append(int(j))

        return arr
