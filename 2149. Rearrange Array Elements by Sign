class Solution:
    def rearrangeArray(self, nums: List[int]) -> List[int]:
        arr1 = []
        arr2 = []
        res = []

        for i in nums:
            if i > 0:
                arr1.append(i)
            else:
                arr2.append(i)

        for i in range(len(arr1)):
            res.append(arr1[i])
            res.append(arr2[i])

        return res
